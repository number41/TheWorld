@model IEnumerable<TheWorld.Models.Trip>
@{ 
    ViewBag.Title = "Home Page";
}

<h2>The World</h2>
<p>This will be a fun website! Maybe</p>

<form>
    <div>
        <label>Date</label>
        <input />
    </div>
    <div>
        <label>Location</label>
        <input />
    </div>
</form>

<ul>
    @foreach (var trip in Model)
    {
        <li>@trip.Name (@trip.Created.ToShortDateString())</li>
    }
</ul>
